#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main() {
    
    char *line = NULL;
    size_t len = 0;
    char *token;
    char *rest;

    printf("Please enter some text:\n");

    getline(&line, &len, stdin);

    line[strcspn(line, "\n")] = 0; // Remove newline character

    token = strtok_r(line, " ", &rest);
    while (token != NULL) {
        printf("%s\n", token);
        token = strtok_r(NULL, " ", &rest);
    }


}

