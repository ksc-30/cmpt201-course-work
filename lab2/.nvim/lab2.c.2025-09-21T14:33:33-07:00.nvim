#include <stdio.h>
#include <stdlib.h>
#include <unistd.h>

int main() {
  char *input = NULL;
  size_t length = 0;

  printf("Enter programs to run");

  getline(&input, &length, stdin);

  char *token = strtok(input, " ");

  while (token != NULL) {
    printf("%s\n", token);
    token = strtok(NULL, " ");
  }
  

  free(input);
}
